<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="gamestock_game_form_inherit" model="ir.ui.view">
			<field name="name">gamestock.game.view.form.inherit</field>
			<field name="model">gamestock.game</field>
			<field name="inherit_id" ref="gamestock.gamestock_game_view_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='name']" position="after">
					<field name="franchise_id"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>